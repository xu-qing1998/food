<view class="box"><view class="main"><view class="title"><image class="logo" src="{{paiInfo.avatar}}"></image><view class="titleName"><view class="tnt">{{paiInfo.username}}</view><view class="tnb">{{paiInfo.dateline}}</view></view></view><view class="texts4">{{paiInfo.title}}</view><view class="texts3">{{paiInfo.subject}}</view><block a:for="{{paiInfo.p800}}" a:for-item="picitem" a:for-index="__i0__" a:key="pic"><view class="imgview"><image class="imglist" src="{{picitem.pic}}" mode="widthFix" data-event-opts="{{[['tap',[['previewImg',['$0'],[[['paiInfo.p800','pic',picitem.pic,'pic']]]]]]]}}" onTap="__e"></image></view></block><block a:if="{{commentList}}"><view class="comment"><view class="discuss">评论</view><block a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="cid"><view class="commentMain"><view style="display:none;">{{''+item.g0+''}}</view><view class="commentTitle"><image class="commentLogo" src="{{item.$orig.avatar}}" data-event-opts="{{[['error',[['onImgError',['$0',index],['commentList']]]]]}}" onError="__e"></image><view class="commentTitleName"><view class="ctnt">{{item.$orig.author}}</view><view class="commentTime"><view class="ctnf">{{item.$orig.floors+"#"}}</view><view class="ctnd">{{item.$orig.dateline}}</view></view></view></view><block a:if="{{item.$orig.cmessage}}"><view class="commentreply">{{"|| "+item.$orig.cmessage}}</view></block><view class="commentContent">{{item.$orig.message}}</view></view></block></view></block><view class="contentBottom">・ End ・</view></view><view class="tabb"><view data-event-opts="{{[['tap',[['chenked',[0]]]]]}}" class="{{(('one')+' '+((isActive==0)?'active':''))}}" onTap="__e"><text class="iconfont icon-pinlun"></text><view class="fsize">评论</view></view><view data-event-opts="{{[['tap',[['chenked',[1]]]]]}}" class="{{(('one')+' '+((isActive==1)?'active':''))}}" onTap="__e"><text class="iconfont icon-shoucang"></text><view class="fsize">收藏</view></view><view data-event-opts="{{[['tap',[['goShare',['$event']]]]]}}" class="one" onTap="__e"><text class="iconfont icon-fenxiang-"></text><view class="fsize">分享</view></view><view data-event-opts="{{[['tap',[['chenked',[3]]]]]}}" class="{{(('one')+' '+((isActive==3)?'active':''))}}" onTap="__e"><text class="iconfont icon-gift-box"></text><view class="fsize">福利社</view></view></view></view>