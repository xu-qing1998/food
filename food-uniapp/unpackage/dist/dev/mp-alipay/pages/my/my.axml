<view><view class="header"><view class="userinfo"><view class="face"><block a:if="{{!hasLogin}}"><button data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" class="sys_btn" onTap="__e"></button></block><block a:if="{{hasLogin}}"><button data-event-opts="{{[['tap',[['bindLogout',['$event']]]]]}}" class="sys_btn" onTap="__e"><image src="../../static/images/xz.jpg"></image></button></block></view></view><view class="setting"><image src="../../static/my/setting.png"></image></view></view><block a:for="{{severList}}" a:for-item="list" a:for-index="list_i" a:key="list_i"><view class="list"><block a:for="{{list}}" a:for-item="li" a:for-index="li_i" a:key="name"><view class="{{(('li')+' '+((li_i==list.length-1)?'noborder':''))}}" hover-class="hover" data-event-opts="{{[['tap',[['toPage',[list_i,li_i]]]]]}}" onTap="__e"><view class="icon"><image src="{{'../../static/my/sever/'+li.icon}}"></image></view><view class="text">{{li.name}}</view><image class="to" src="../../static/my/to.png"></image></view></block></view></block></view>