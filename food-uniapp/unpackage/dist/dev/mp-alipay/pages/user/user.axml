<view class="content"><view class="btn-row"><block a:if="{{!hasLogin}}"><button class="primary" type="primary" data-event-opts="{{[['tap',[['bindLogin',['$event']]]]]}}" onTap="__e">登录</button></block><block a:if="{{hasLogin}}"><button type="default" data-event-opts="{{[['tap',[['bindLogout',['$event']]]]]}}" onTap="__e">退出登录</button></block></view></view>