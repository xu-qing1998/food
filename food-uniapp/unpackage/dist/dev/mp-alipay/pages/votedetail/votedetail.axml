<view class="box"><view class="main"><view class="title"><image class="logo" src="{{paiInfo.avatar}}" mode></image><view class="titleName"><view class="tnt">{{paiInfo.username}}</view><view class="tnb">{{paiInfo.dateline}}</view></view></view><view class="content"><view class="titletop">{{voteInfo.title}}</view><image src="{{paiInfo.p800[0].pic}}" mode="widthFix"></image></view><view class="radio"><view class="uni-title uni-common-mt uni-common-pl">{{voteInfo.title+" (单选)"}}</view><view class="uni-list"><radio-group data-event-opts="{{[['tap',[['changeRadio',['$event']]]]]}}" onTap="__e"><block a:for="{{voteInfo.optionlist}}" a:for-item="item" a:for-index="__i0__" a:key="id"><label class="list"><view class="radioValue"><radio class="radioo" value="{{item.id}}" ref="__r" data-ref-in-for="radio_ref"></radio></view><view class="radioTitle">{{item.title}}</view></label></block></radio-group><button ref="__r" data-ref="button" data-event-opts="{{[['tap',[['chenked']]]]}}" onTap="__e">投 票</button></view></view><view class="comment"><block a:if="{{commentList!=null}}"><view class="discuss">评论</view></block><block a:else><view class="discuss"></view></block><block a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="cid"><view class="commentMain"><view style="display:none;">{{''+item.g0+''}}</view><view class="commentTitle"><image class="commentLogo" src="{{item.$orig.avatar}}" data-event-opts="{{[['error',[['onImgError',['commentList',index]]]]]}}" onError="__e"></image><view class="commentTitleName"><view class="ctnt">{{item.$orig.author}}</view><view class="commentTime"><view class="ctnf">{{item.$orig.floors+"#"}}</view><view class="ctnd">{{item.$orig.dateline}}</view></view></view></view><view class="commentContent">{{item.$orig.message}}</view></view></block></view><block a:if="{{!flag}}"><uni-load-more vue-id="1" status="loading" onVueInit="__l"></uni-load-more></block><block a:else><uni-load-more vue-id="2" status="noMore" onVueInit="__l"></uni-load-more></block></view><view class="tabb"><view data-event-opts="{{[['tap',[['goComment',['$event']]]]]}}" class="one" onTap="__e"><text class="iconfont icon-pinlun"></text><view class="fsize">评论</view></view><view data-event-opts="{{[['tap',[['goCollect',['$event']]]]]}}" class="one" style="{{(flag1?'color:red':'')}}" onTap="__e"><text class="iconfont icon-shoucang"></text><view class="fsize">收藏</view></view><view data-event-opts="{{[['tap',[['goShare',['$event']]]]]}}" class="one" onTap="__e"><text class="iconfont icon-fenxiang-"></text><view class="fsize">分享</view></view><view data-event-opts="{{[['tap',[['goGift',['$event']]]]]}}" class="one" style="color:red;" onTap="__e"><text class="iconfont icon-gift-box"></text><view class="fsize">福利社</view></view></view></view>