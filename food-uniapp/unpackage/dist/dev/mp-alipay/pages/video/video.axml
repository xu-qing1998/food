<view class="box"><block a:for="{{arr}}" a:for-item="item" a:for-index="index" a:key="courseId"><view class="title"><image src="{{item.arr1.courseImage}}" mode="scaleToFill" data-index="{{index}}" data-item="{{item}}" data-event-opts="{{[['tap',[['goSuperMarket',[index]]]]]}}" onTap="__e"></image><view class="box_bottom"><view class="fl"><view class="fl_top">{{item.arr1.courseTitle}}</view><view class="fl_bottom">{{item.arr1.courseTitle}}</view></view><view class="fr"><text class="sc iconfont icon-ziyuanldpi" style="{{(item.flag?'color:red':'')}}" disabled="disabled" data-event-opts="{{[['tap',[['addone',[index]]]]]}}" onTap="__e"></text>{{item.arr1.collectCount+'个收藏'}}</view></view></view></block><block a:if="{{!aflag}}"><uni-load-more vue-id="1" status="loading" onVueInit="__l"></uni-load-more></block><block a:else><uni-load-more vue-id="2" status="noMore" onVueInit="__l"></uni-load-more></block></view>