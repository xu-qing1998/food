<view><uni-search-bar vue-id="1" placeholder="今天想吃点啥？" radius="100" value="{{name}}" data-event-opts="{{[['^confirm',[['search']]],['^input',[['__set_model',['','name','$event',[]]]]]]}}" onConfirm="__e" onInput="__e" onVueInit="__l"></uni-search-bar><view class="recommend">热门搜索</view><view class="btns"><button class="btn" type="default" data-event-opts="{{[['tap',[['searchOne',['$0'],['foodsList.__$n0']]]]]}}" onTap="__e">{{foodsList[0]}}</button><button class="btn" type="default" data-event-opts="{{[['tap',[['searchOne',['$0'],['foodsList.__$n1']]]]]}}" onTap="__e">{{foodsList[1]}}</button><button class="btn" type="default" data-event-opts="{{[['tap',[['searchOne',['$0'],['foodsList.__$n2']]]]]}}" onTap="__e">{{foodsList[2]}}</button><button class="btn" type="default" data-event-opts="{{[['tap',[['searchOne',['$0'],['foodsList.__$n3']]]]]}}" onTap="__e">{{foodsList[3]}}</button><button class="btn" type="default" data-event-opts="{{[['tap',[['searchOne',['$0'],['foodsList.__$n4']]]]]}}" onTap="__e">{{foodsList[4]}}</button></view><block a:for="{{foodList2}}" a:for-item="item" a:for-index="__i0__" a:key="name"><view data-event-opts="{{[['tap',[['toFoodDetail',['$0'],[[['foodList2','name',item.name]]]]]]]}}" onTap="__e"><view class="img"><image src="{{item.imgSmall}}" mode></image><view class="text"><view>{{item.name}}</view><view>{{item.collectCount+"人收藏"}}</view></view></view></view></block><block a:for="{{foodList4}}" a:for-item="item" a:for-index="__i1__" a:key="name"><view data-event-opts="{{[['tap',[['toFoodDetail',['$0'],[[['foodList4','name',item.name]]]]]]]}}" onTap="__e"><view class="img"><image src="{{item.imgSmall}}" mode></image><view class="text"><view>{{item.name}}</view><view>{{item.collectCount+"人收藏"}}</view></view></view></view></block><block a:if="{{!flag}}"><view>抱歉，暂时没有您要找的菜呢。</view></block></view>