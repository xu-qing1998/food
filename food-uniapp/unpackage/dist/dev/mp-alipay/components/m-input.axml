<view class="m-input-view"><input class="m-input-input" focus="{{focus_}}" type="{{inputType}}" placeholder="{{placeholder}}" password="{{type==='password'&&!showPassword}}" data-event-opts="{{[['input',[['onInput',['$event']]]],['focus',[['onFocus',['$event']]]],['blur',[['onBlur',['$event']]]]]}}" value="{{value}}" onInput="__e" onFocus="__e" onBlur="__e"/><block a:if="{{clearable_&&!displayable_&&value.length}}"><view class="m-input-icon"><m-icon vue-id="1" color="#666666" type="clear" size="20" data-event-opts="{{[['^click',[['clear']]]]}}" onClick="__e" onVueInit="__l"></m-icon></view></block><block a:if="{{displayable_}}"><view class="m-input-icon"><m-icon vue-id="2" color="{{showPassword?'#666666':'#cccccc'}}" type="eye" size="20" data-event-opts="{{[['^click',[['display']]]]}}" onClick="__e" onVueInit="__l"></m-icon></view></block></view>