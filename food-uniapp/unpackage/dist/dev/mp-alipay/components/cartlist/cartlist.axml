<view><view class="container"><block a:for="{{listData}}" a:for-item="item" a:for-index="index" a:key="index"><view class="slide-box"><view data-event-opts="{{[['touchStart',[['touchStart',['$event',index]]]],['touchEnd',[['touchEnd',['$event',index]]]],['touchMove',[['touchMove',['$event',index]]]],['tap',[['recover',[index]]]]]}}" class="slide-list" style="{{'transform:'+('translate3d('+item.slide_x+'px, 0, 0)')+';'}}" onTouchStart="__e" onTouchEnd="__e" onTouchMove="__e" onTap="__e"><view class="now-message-info" style="{{'width:'+(windowWidth+'px')+';'}}" hover-class="uni-item--hover" data-event-opts="{{[['tap',[['clickItemMethod',['$0'],[[['listData','',index]]]]]]]}}" onTap="__e"><block a:if="{{item.imgPath}}"><image class="icon-image" src="{{item.imgPath}}"></image></block><view class="list-right"><block a:if="{{item.name}}"><view class="list-title">{{item.name}}</view></block><block a:if="{{item.price}}"><view class="list-detail">{{"参考价：￥"+item.price}}</view></block></view></view><view class="group-btn"><block a:for="{{button}}" a:for-item="value" a:for-index="key" a:key="key"><view data-event-opts="{{[['tap',[['clickMethod',['$0','$1',index],[[['listData','',index]],[['button','',key]]]]]]]}}" class="btn-div" style="{{'background:'+(value.background)+';'}}" onTap="__e">{{value.title}}</view></block></view><view style="clear:both;"></view></view><block a:if="{{border}}"><view class="list-item-border"></view></block></view></block></view></view>