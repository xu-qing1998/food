<view class="uni-popup-dialog data-v-20e430e4"><view class="uni-dialog-title data-v-20e430e4"><text class="{{(('uni-dialog-title-text data-v-20e430e4')+' '+('uni-popup__'+dialogType))}}">{{title}}</text></view><view class="uni-dialog-content data-v-20e430e4"><block a:if="{{mode==='base'}}"><text class="uni-dialog-content-text data-v-20e430e4">{{content}}</text></block><block a:else><input class="uni-dialog-input data-v-20e430e4" type="text" placeholder="请输入内容" focus="{{focus}}" data-event-opts="{{[['input',[['__set_model',['','val','$event',[]]]]]]}}" value="{{val}}" onInput="__e"/></block></view><view class="uni-dialog-button-group data-v-20e430e4"><view data-event-opts="{{[['tap',[['close',['$event']]]]]}}" class="uni-dialog-button data-v-20e430e4" onTap="__e"><text class="uni-dialog-button-text data-v-20e430e4">取消</text></view><view data-event-opts="{{[['tap',[['onOk',['$event']]]]]}}" class="uni-dialog-button uni-border-left data-v-20e430e4" onTap="__e"><text class="uni-dialog-button-text uni-button-color data-v-20e430e4">确定</text></view></view></view>