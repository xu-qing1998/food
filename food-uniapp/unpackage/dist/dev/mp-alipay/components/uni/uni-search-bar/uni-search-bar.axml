<view class="uni-searchbar data-v-d1b84912"><view data-event-opts="{{[['tap',[['searchClick',['$event']]]]]}}" class="uni-searchbar__box data-v-d1b84912" style="{{'border-radius:'+(radius+'px')+';'+('background-color:'+(bgColor)+';')}}" onTap="__e"><view class="uni-searchbar__box-icon-search data-v-d1b84912"><uni-icons vue-id="1" color="#999999" size="18" type="search" class="data-v-d1b84912" onVueInit="__l"></uni-icons></view><block a:if="{{show}}"><input class="uni-searchbar__box-search-input data-v-d1b84912" focus="{{showSync}}" placeholder="{{placeholder}}" maxlength="{{maxlength}}" confirm-type="search" type="text" data-event-opts="{{[['confirm',[['confirm',['$event']]]],['input',[['__set_model',['','searchVal','$event',[]]]]]]}}" value="{{searchVal}}" onConfirm="__e" onInput="__e"/></block><block a:else><text class="uni-searchbar__text-placeholder data-v-d1b84912">{{placeholder}}</text></block><block a:if="{{show&&(clearButton==='always'||clearButton==='auto'&&searchVal!=='')}}"><view data-event-opts="{{[['tap',[['clear',['$event']]]]]}}" class="uni-searchbar__box-icon-clear data-v-d1b84912" onTap="__e"><uni-icons vue-id="2" color="#999999" size="24" type="clear" class="data-v-d1b84912" onVueInit="__l"></uni-icons></view></block></view><block a:if="{{cancelButton==='always'||show&&cancelButton==='auto'}}"><text data-event-opts="{{[['tap',[['cancel',['$event']]]]]}}" class="uni-searchbar__cancel data-v-d1b84912" onTap="__e">{{cancelText}}</text></block></view>