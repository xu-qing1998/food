<view><view class="continer"><view class="title">{{themeName}}</view><view class="themeIntroduce">{{themeDescription}}</view><view class="title2">推荐菜</view><block wx:for="{{themeList}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="buttons"><button class="button_item" type="default" data-event-opts="{{[['tap',[['goFoodDetails',['$0'],[[['themeList','id',item.id]]]]]]]}}" bindtap="__e">{{item.name}}</button></view></block><block wx:for="{{themeList}}" wx:for-item="item" wx:for-index="__i1__" wx:key="id"><view class="foodsPhoto"><view class="Photo_item"><image src="{{item.imgLarge}}" data-event-opts="{{[['tap',[['goFoodDetails',['$0'],[[['themeList','id',item.id]]]]]]]}}" bindtap="__e"></image><view class="text"><text class="left">{{item.name}}</text><view class="right"><uni-icons vue-id="{{'1-'+__i1__}}" type="heart" size="20" bind:__l="__l"></uni-icons><text class="collect">{{item.collectCount+"人收藏"}}</text></view></view></view></view></block><view class="blank"></view><view class="bottom"><view data-event-opts="{{[['tap',[['toIndex',['$event']]]]]}}" class="tab_item" bindtap="__e"><uni-icons vue-id="2" type="home" size="20" bind:__l="__l"></uni-icons><view class="text">首页</view></view><view class="tab_item"><uni-icons vue-id="3" type="redo" size="20" bind:__l="__l"></uni-icons><view class="btn"><button class="share-btn text" open-type="share">分享</button></view></view></view></view></view>